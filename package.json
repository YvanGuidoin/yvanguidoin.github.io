{
  "name": "YvanGuidoin",
  "version": "0.2.0",
  "description": "Resume for Yvan Guidoin",
  "main": "main.bundle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YvanGuidoin/yvanguidoin.github.io"
  },
  "author": "YvanGuidoin <yvan.guidoin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --colors --config webpack.config.js --env.production=production",
    "build:watch": "webpack --colors -w --config webpack.config.js --env.production=development",
    "lint": "eslint ./src/**/*.jsx",
    "live": "live-server --port=8080 --ignore=node_modules,.git,src --wait=2000 --no-browser -q",
    "start": "concurrently \"npm run live\" \"npm run build:watch\"",
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "watch": "concurrently \"npm run build:watch\" \"npm run test:watch\""
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "enzyme",
      "react",
      "react-bootstrap",
      "react-dom"
    ]
  },
  "dependencies": {
    "bootstrap": "3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.10",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-waypoint": "^6.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "live-server": "^1.2.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.2",
    "node-sass": "^4.5.2",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1"
  }
